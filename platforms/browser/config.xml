<?xml version='1.0' encoding='utf-8'?>
<widget id="com.studio101.rombongsedekah" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Rombong Sedekah</name>
    <description>
        Aplikasi untuk donatur Rombong Sedekah merencanakan donasinya.
    </description>
    <author email="awalsadja@gmail.com" href="http://cordova.io">
        Studio 101
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <splash src="splash.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Fullscreen" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <preference name="SplashScreen" value="res/screen/android/screen-mdpi-portrait.png" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="SplashScreenBackgroundColor" value="#00923f" />
    <preference name="ShowSplashScreen" value="true" />
    <preference name="SplashScreenWidth" value="320" />
    <preference name="SplashScreenHeight" value="470" />
</widget>
